<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:color="@color/mid">
        <shape>
            <corners android:radius="13dp"/>
            <solid android:color="@color/mid"/>
            <stroke
                android:width="1dp"
                android:color="#33FFFFFF"/>
        </shape>
    </item>

    <item android:color="@color/defaultPrioColor">
        <shape>
            <corners android:radius="13dp"/>
            <solid android:color="@color/defaultPrioColor"/>
            <stroke
                android:width="1dp"
                android:color="#33FFFFFF"/>
        </shape>
    </item>
</selector>